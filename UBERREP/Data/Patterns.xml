  </ValidationItem>
  <ValidationItem>
    <Pattern>\A[a-zA-Z\s\-\.\']{2,50}\z</Pattern>
    <Type>City</Type>
  </ValidationItem>
  <ValidationItem>
    <Pattern>^[a-zA-Z][\w\.-]*[a-zA-Z0-9]@[a-zA-Z0-9][\w\.-]*[a-zA-Z0-9]\.[a-zA-Z][a-zA-Z\.]*[a-zA-Z]$</Pattern>
