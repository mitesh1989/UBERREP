<?xml version="1.0"?>
<ArrayOfValidationItem>
  <ValidationItem>
    <Pattern>\A[a-zA-Z0-9_\s\-\.\#\'\,\@\*\\&amp;/\\]{4,500}\z</Pattern>
    <Type>Address</Type>
  </ValidationItem>
  <ValidationItem>
    <Pattern>\A[0-9\-\+]{1,}\z</Pattern>
    <Type>Int16</Type>
  </ValidationItem>
  <ValidationItem>
    <Pattern>\A[0-9\.\-]{1,}\z</Pattern>
    <Type>Decimal</Type>
  </ValidationItem>
  <ValidationItem>
    <Pattern>\A[0-9\,]{3,50}\z</Pattern>
    <Type>Phone</Type>
  </ValidationItem>
  <ValidationItem>
    <Pattern>\A[0-9\,]{10,50}\z</Pattern>
    <Type>Mobile</Type>
  </ValidationItem>
  <ValidationItem>
    <Pattern>\A[0-9\s\-\.\(\)\+]{5,50}\z</Pattern>
