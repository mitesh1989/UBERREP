  </ValidationItem>
  <ValidationItem>
    <Pattern>\A[123456789101112]{1,2}\z</Pattern>
    <Type>Month</Type>
  </ValidationItem>
  <ValidationItem>
    <Pattern>^(?:(?:(?:0?[13578]|1[02])(\/|-|\.)31)\1|(?:(?:0?[13-9]|1[0-2])(\/|-|\.)(?:29|30)\2))(?:(?:1[6-9]|[2-9]\d)?\d{2})$|^(?:0?2(\/|-|\.)29\3(?:(?:(?:1[6-9]|[2-9]\d)?(?:0[48]|[2468][048]|[13579][26])|(?:(?:16|[2468][048]|[3579][26])00))))$|^(?:(?:0?[1-9])|(?:1[0-2]))(\/|-|\.)(?:0?[1-9]|1\d|2[0-8])\4(?:(?:1[6-9]|[2-9]\d)?\d{2})$</Pattern>
    <Type>Date</Type>
  </ValidationItem>  
  <ValidationItem>
    <Pattern>\A[A-Za-z_\,\'\-\s]{2,50}\z</Pattern>
    <Type>Name</Type>
  </ValidationItem>
  <ValidationItem>
    <Pattern>^[a-zA-Z][\w\.-]*[a-zA-Z0-9]@[a-zA-Z0-9][\w\.-]*[a-zA-Z0-9]\.[a-zA-Z][a-zA-Z\.]*[a-zA-Z]$</Pattern>
    <Type>Username</Type>
  </ValidationItem>
  <ValidationItem>
    <Pattern>\A[A-Za-z0-9_\s\-\@\!\$]{5,50}\z</Pattern>
    <Type>Password</Type>
  </ValidationItem>
  <ValidationItem>
    <Pattern>\A[A-Za-z]{2,15}\z</Pattern>
    <Type>CategoryCode</Type>
  </ValidationItem>
  <ValidationItem>
    <Pattern>\A[A-Z0-9]{3,10}\z</Pattern>
    <Type>ClientID</Type>
  </ValidationItem>
  <ValidationItem>
    <Pattern>\A[A-Za-z0-9]{3,10}\z</Pattern>
    <Type>ZipCode</Type>
  </ValidationItem>
